{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Deislabs.WAGI": "Trace"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:8080"
            }
        }
    },
    "Wagi": {
        "ModulePath": "modules",
        "Modules": {
            "rubywasm": {
                "FileName": "ruby.wasm",
                "Volumes": {
                    "/": "lib",
                    "/usr": "ruby-wasm32-wasi/usr"
                },
                "Route": "/"
            }
        }
    }
}